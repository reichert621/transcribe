    "webpack-hot-middleware": "2.22.1"
  },
  "engines": {
    "node": "8.11.0"
  },
  "jest": {
    "rootDir": "client",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run prettier --",
      "git add"
    ]
  },
  "main": "server/index.js",
  "scripts": {
    "heroku-postbuild": "npm run db:migrate && npm run ts:build && npm run build",
    "start": "node server/build/index.js",
    "js:start": "node server/index.js",
    "ts:build": "tsc --project server/tsconfig.json",
    "ts:start": "ts-node --project server/tsconfig.json server/index.ts",
    "ts:dev": "ts-node-dev --respawn --transpileOnly server/index.ts",
    "dev": "./node_modules/nodemon/bin/nodemon.js server/index.js",
    "eslint:client": "./node_modules/.bin/eslint 'client/app/**/*.js'",
    "tslint:client": "./node_modules/.bin/tslint 'client/app/**/*.ts'",
    "lint:client": "npm run tslint:client && npm run eslint:client",
    "lint:api": "./node_modules/.bin/eslint 'server/**/*.js'",
    "lint": "./node_modules/.bin/eslint",
    "prettier:client": "./node_modules/.bin/prettier --write 'client/**/*.{js,ts,tsx}'",
    "prettier:api": "./node_modules/.bin/prettier --write 'server/**/*.{js,ts,tsx}'",
    "prettier": "npm run prettier:client && npm run prettier:api",
    "db:migration": "./node_modules/.bin/knex migrate:make --knexfile server/db/knexfile.js",
    "db:migrate": "./node_modules/.bin/knex migrate:latest --knexfile server/db/knexfile.js",
    "db:seed": "./node_modules/.bin/knex seed:run --knexfile server/db/knexfile.js",
    "db:rollback": "./node_modules/.bin/knex migrate:rollback --knexfile server/db/knexfile.js",
    "build": "./node_modules/.bin/webpack --config webpack.prod.config.js -p",
    "watch": "./node_modules/.bin/webpack --config webpack.dev.config.js -w",
    "test:api": "./node_modules/.bin/mocha --require ts-node/register server/tests/**/*.ts",
    "test:ui": "jest"
  }
}
